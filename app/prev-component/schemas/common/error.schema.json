{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "ErrorResponse",
  "type": "object",
  "properties": {
    "timestamp": { "type": ["string", "integer"] },
    "status": { "type": "integer" },
    "error": { "type": "string" },
    "message": { "type": "string" },
    "path": { "type": "string" },
    "code": { "type": ["integer", "string"] },
    "cause": {
      "type": "object",
      "properties": {
        "code": { "type": ["integer", "string"] },
        "msg": { "type": "string" }
      }
    }
  },
  "required": ["status", "message"]
}
